use io
use conv

class Point {
  move(dx: int, dy: int)
  add(p: Point): Point
  coords(): int, int
  clone(): Point

  // Initialize this to contain (x,y).
  // Requires: p is uninitialized.
  // Returns: this
  initPoint(x: int, y: int): Point

  equals(p: Point): bool
  toString(): int[]
}

// Create the point (x,y).
createPoint(x: int, y: int): Point
